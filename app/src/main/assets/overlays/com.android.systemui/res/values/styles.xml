<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="oneplus_contorl_text_style_body1" parent="@*com.android.systemui:style/oneplus_contorl_text_style_base">
        <item name="android:textSize">11.0sp</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:breakStrategy">simple</item>
    </style>
    <style name="TextAppearance.Keyguard.Secondary" parent="@*com.android.systemui:style/TextAppearance.Keyguard">
        <item name="android:textSize">@*com.android.systemui:dimen/widget_label_font_size</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:lines">1</item>
        <item name="android:fontFamily">google-sans</item>
    </style>
    <style name="widget_big_thin">
        <item name="android:textSize">@*com.android.systemui:dimen/widget_big_font_size</item>
        <item name="android:paddingBottom">@*com.android.systemui:dimen/bottom_text_spacing_digital</item>
        <item name="android:fontFamily">google-sans</item>
        <item name="android:fontFeatureSettings">ss03</item>
    </style>
    <style name="TextAppearance.StatusBar.Clock" parent="@android:style/TextAppearance.StatusBar.Icon">
        <item name="android:textSize">@*com.android.systemui:dimen/status_bar_clock_size</item>
        <item name="android:textColor">@*com.android.systemui:color/status_bar_clock_color</item>
        <item name="android:fontFamily">google-sans-medium</item>
    </style>
    <style name="TextAppearance.StatusBar.Expanded.Clock" parent="@*com.android.systemui:style/TextAppearance.StatusBar.Expanded">
        <item name="android:textSize">@*com.android.systemui:dimen/qs_time_expanded_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:fontFamily">google-sans-medium</item>
    </style>
